{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client", "node"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "resolveJsonModule": true
  }
}